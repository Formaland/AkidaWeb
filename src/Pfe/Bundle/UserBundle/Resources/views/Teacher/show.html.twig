
{% block body -%}
    <h1>{{ 'sidebar.Liste des apprenants'|trans({}, 'backend') }}</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            {# <tr>
                <th>User</th>
                <td>{{ entity.user }}</td>
            </tr>#}
            <tr>
                <th>Diplome</th>
                <td>{{ entity.diplome }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('teacher') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('teacher_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
